<% render '/users/errors' %>
<div class= "whiteboard">
<center><h2>Enter Store Code</h2></center>
    <center><%= form_with model: @user do |f| %>
    <p>Access Num -- StoreID -- EmployeeID<br></p>
    <%= f.password_field :password%><br>
    <%= f.text_field :store_id %>
    <%= f.text_field :employeeInit %><br>

    ReEnter Access Number.<br>
    <%= f.password_field :password_confirmation %><br>
    Email :
        <%= f.email_field :email%><br>
    Telephone Number :        
    <%= f.text_field :telephone %>
    <%= f.submit "OK"%>
 <%end%> 
 </center>
</div>